---
import Header from '../components/Header.astro';
import Layout from '../layouts/Layout.astro';
import '../styles/global.css';
---

<Layout title="Nominate">
    <Header/>
	<main>
		<div class="container">	  
			<div>
			  <iframe
				id="typeform-full"
				width="100%"
				height="100%"
				allow="camera; microphone; autoplay; encrypted-media;"
				src="https://docs.google.com/forms/d/e/1FAIpQLSd7AYocBRlSRoXtIGsitr_eO5dgbJ7tLd7BoDzLrq_-qAbNPg/viewform?embedded=true"
			  ></iframe>
			</div>
	  

			  <a href="/" class="auxNav arrowback nominate">‚Üê</a>
	</main>
	<style>
		html,
        body {
          margin: 0;
          height: 100%;
          overflow: hidden;
        }
        .container {
          min-height: 100vh;
        }
        iframe {
          position: fixed;
          left: 0;
          right: 0;
          bottom: 0;
          top: 0;
          border: 0;
        }
	</style>
</Layout>

